{
    "version": "0.2.0",
    "configurations": [
      {
        "type": "node",
        "request": "attach",
        "name": "Attach to SAM CLI",
        "address": "localhost",
        "port": 5858,
        "localRoot": "${workspaceRoot}/packages/examples/dist",
        "outFiles": ["${workspaceFolder}/packages/examples/dist/**/*.js"],
        "remoteRoot": "/var/task",
        "protocol": "inspector",
        "stopOnEntry": false
      }
    ]
  }